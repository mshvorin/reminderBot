{
  "family": "discord-bot-task",
  "executionRoleArn": "arn:aws:iam::965432218846:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::965432218846:role/ecsTaskRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mongoReminder-container",
      "image": "965432218846.dkr.ecr.us-east-1.amazonaws.com/discord-bot:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "environment": [
        {
          "name": "DISCORD_TOKEN",
          "value": "${DISCORD_TOKEN}"
        },
        {
          "name": "MONGODB_URI",
          "value": "${MONGODB_URI}"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/discord-bot-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
